{
  "name": "phazerous",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typeorm": "ts-node -r tsconfig-paths/register -P ./tsconfig.typeorm.json ./node_modules/typeorm/cli.js -d src/config/db.ts schema:sync",
    "db:sync": "npm run typeorm ",
    "db": "npm run typeorm migration:run -d src/config/db.ts",
    "db:migrate:local": "typeorm migration:run",
    "db:init": "typeorm init --name my-project --database sqlite",
    "db:entity": "typeorm entity:create src/entities/name",
    "db:generate": "typeorm-ts-node-commonjs migration:generate src/migration -d src/config/db.ts",
    "dbtest": "typeorm-ts-node-commonjs schema:sync -d src/config/db.ts "
  },
  "dependencies": {
    "@types/node": "20.4.6",
    "@types/react": "18.2.18",
    "@types/react-dom": "18.2.7",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.12",
    "next": "13.4.12",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.1.6",
    "@refinedev/antd": "^5.32.0",
    "@refinedev/core": "^4.32.0",
    "@refinedev/cli": "^2.7.4",
    "@refinedev/react-router-v6": "^4.5.0",
    "@refinedev/simple-rest": "^4.5.0",
    "@refinedev/inferencer" : "^4.5.0",
    "@refinedev/nextjs-router": "^5.5.0",
    "@uiw/react-md-editor": "^3.19.5",
    "@ant-design/icons": "5.0.1",
    "antd": "^5.0.5",
    "react-router-dom": "^6.8.1",
    "typeorm": "^0.3.17",
    "sqlite3": "^5.1.6",
    "@tanstack/react-query": "5.0.0-beta.9",
    "@tanstack/react-query-devtools": "5.0.0-beta.9",
    "reflect-metadata": "^0.1.13",
    "@babel/plugin-proposal-decorators": "7.21.4-esm.4",
    "mysql2": "^3.5.2",
    "sass": "^1.64.2",
    "next-rest-framework" : "^0.8.0"
  },
  "devDependencies": {
    "@types/node": "^18.16.2",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "ts-node": "^10.9.1"
  }
}
